(version 1)
(rule "BGA neckdown"
    (constraint track_width (min 3mil) (opt 4.2mil))
    (constraint clearance (min 3mil) (opt 4.5mil))
    (condition "A.enclosedByArea('BGA')"))
(rule "Differential pair clearance"
    (condition "A.inDiffPair('*') && !AB.isCoupledDiffPair() && B.NetName!='GND' && B.Type != 'Pad' && !A.intersectsArea('BGA')")
    (constraint clearance (min 12.75mil)))
